<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
    <mapper namespace="mapper.member">
    
    <!-- member login -->
	<select id="login" parameterType="com.hotel.vo.MemberVO" resultType="com.hotel.vo.SessionVO">
		select count(*) loginresult, mid from member where mid=#{mid} group by mid
	</select>
    
    </mapper>